{
  "name": "iconfont-core",
  "version": "0.1.0",
  "description": "提供制作iconfont的核心方法：normalizeSVGPath 和 generateFont。",
  "main": "./src/index.js",
  "bin": {
    "iconfont": "./bin/iconfont.js"
  },
  "keywords": [
    "icon",
    "font",
    "iconfont",
    "svg"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Z8264/iconfont-core"
  },
  "license": "MIT",
  "scripts": {
    "iconfont": "node ./bin/iconfont.js",
    "test": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testMatch": [
      "**/test/*.test.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "bail": true,
    "testEnvironment": "node"
  },
  "author": "zhangbo",
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "findup-sync": "^3.0.0",
    "glob": "^7.1.3",
    "sax": "^1.2.4",
    "svg2ttf": "^4.1.0",
    "svgpath": "^2.2.1",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.1",
    "ttf2woff2": "^2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.4",
    "jest": "^29.3.1",
    "lint-staged": "^8.0.4"
  }
}
